<header>

  <div class="container">

    <div class="profile">

      <div class="profile-image">

        <img
          src="/upload/{{user.my_picture}}"
          alt=""
        />

      </div>

      <div class="profile-user-settings">

        <h1 class="profile-user-name">{{user.name}}</h1>

        
        <td class="text-center"><button class="btn profile-edit-btn"><a class="editProfile" href="/profile/upload">EditPic</a></button>

        <button
          class="btn profile-settings-btn"
          aria-label="profile settings"
        ><i class="fas fa-cog" aria-hidden="true"><a href="/logout">Logout</a></i></button>
        

      </div>

      <div class="profile-stats">

        <ul>
          <li><span class="profile-stat-count">164</span> posts</li>
          <li><span class="profile-stat-count">188</span> followers</li>
          <li><span class="profile-stat-count">206</span> following</li>
        </ul>

      </div>

      <div class="profile-bio">

        <p><span class="profile-real-name">{{user.name}}</span>
          Travel, Coding, Family, Eat, Cook üì∑‚úàÔ∏èüèïÔ∏è</p>

      </div>

    </div>
   

  </div>
 

</header>


<form class="upload-form" action="/profile" method="POST" enctype="multipart/form-data">
<div id="hide" class="col-lg-8 col-xs-8">
   <label class="hand-cursor">
    <input name="pictures" type="file" nv-file-select uploader="$ctrl.uploader" multiple/>
    <span class="fa fa-camera"></span>
    <span class="photo_text hidden-xs"></span>
     </label>
     <button type="submit" class="btn btn-success">.</button>
</div>
</form>



<main>
   {{#if all_pics}}
           {{#each all_pics}}
  
	<div class="container">

    
		<div class="gallery">
     

			<div class="gallery-item">
        
 
        

				<img src="/upload/{{this.filename}}" class="gallery-image" alt="">

				<div class="gallery-item-info">

					<ul>
						<li class="gallery-item-likes"><span class="visually-hidden">Likes:</span><i class="fas fa-heart" aria-hidden="true"></i> 56</li>
						<li class="gallery-item-comments"><span class="visually-hidden">Comments:</span><i class="fas fa-comment" aria-hidden="true"></i> 2</li>
             <td class="text-center"><button class="btn btn-danger"><a href="/profile/delete/{{_id}}">Delete</a></button>
					</ul>

          
         
    

				</div>
     
       

			</div>
          
    
  </div>
    {{/each}}
    {{/if}}
     
</main>





